<h1>New panel</h1>

<% form_for(@panel) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :panel_group_id %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', panels_path %>
